    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Build and run debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/ralph/directZob/DirectZobExe",
            "args": [ 
                "--bench",
                "--scene",
                "./resources/fbx.xml",
                "--width",
                "640",
                "--height",
                "400"
            ],
            "stopAtEntry": false,
            "cwd": "/home/ralph/directZob/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build debug",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Build and run release",
            "request": "launch",
            "program": "/home/ralph/directZob/DirectZobExe",
            "args": [
                "--bench",
